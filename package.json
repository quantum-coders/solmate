{
  "name": "nuxt-app",
  "private": true,
  "engines": {
    "node": "16.x"
 },
  "scripts": {
    "start:dev": "node .output/server/index.mjs",
    "start:backend": "cd ./backend && yarn pm2 -f start index.js",
    "start:ai": "cd ./backend/ai-service && flask run",
    "start": "concurrently \"yarn start:dev\" \"yarn start:backend\" \"yarn start:ai\"",
    "build": "nuxt build",
    "dev": "nuxt dev --port=3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@mdi/js": "^7.1.96",
    "concurrently": "^8.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "nuxt": "^3.0.0"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "@solana/wallet-adapter-base": "^0.9.22",
    "@solana/wallet-adapter-solflare": "^0.6.24",
    "@solana/wallet-adapter-walletconnect": "^0.1.14",
    "@solana/web3.js": "^1.77.3",
    "buffer": "^6.0.3",
    "material-design-icons-iconfont": "^6.7.0",
    "pm2": "^5.3.0",
    "sass": "^1.57.1",
    "vuetify": "^3.1.2"
  }
  
}
